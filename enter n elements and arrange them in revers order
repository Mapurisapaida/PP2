#include <stdio.h>
#include <conio.h>

#define MAX 100

void main()
{
    int arr[MAX], n, i;

    clrscr();

    printf("Enter the number of elements (max %d): ", MAX);
    scanf("%d", &n);

    printf("Enter %d elements:\n", n);
    for(i = 0; i < n; i++)
    {
        scanf("%d", &arr[i]);
    }

    printf("\nElements in reverse order are:\n");
    for(i = n - 1; i >= 0; i--)
    {
        printf("%d ", arr[i]);
    }

    getch();
}
