#include <stdio.h>
#include <conio.h>

void main()
{
    int a[3][3], b[3][3], diff[3][3];
    int i, j;

    clrscr();

    printf("Enter elements of first 3x3 matrix:\n");
    for(i = 0; i < 3; i++)
    {
        for(j = 0; j < 3; j++)
        {
            scanf("%d", &a[i][j]);
        }
    }

    printf("\nEnter elements of second 3x3 matrix:\n");
    for(i = 0; i < 3; i++)
    {
        for(j = 0; j < 3; j++)
        {
            scanf("%d", &b[i][j]);
        }
    }

   
    for(i = 0; i < 3; i++)
    {
        for(j = 0; j < 3; j++)
        {
            diff[i][j] = a[i][j] - b[i][j];
        }
    }

    printf("\nDifference of the two matrices is:\n");
    for(i = 0; i < 3; i++)
    {
        for(j = 0; j < 3; j++)
        {
            printf("%d ", diff[i][j]);
        }
        printf("\n");
    }

    getch();
}
