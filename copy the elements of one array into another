#include <stdio.h>
#include <conio.h>

#define MAX 100

void main()
{
    int arr1[MAX], arr2[MAX], n, i;

    clrscr();

    printf("Enter the number of elements (max %d): ", MAX);
    scanf("%d", &n);

    printf("Enter %d elements:\n", n);
    for(i = 0; i < n; i++)
    {
        scanf("%d", &arr1[i]);
    }

  
    for(i = 0; i < n; i++)
    {
        arr2[i] = arr1[i];
    }

    printf("\nElements of second array are:\n");
    for(i = 0; i < n; i++)
    {
        printf("%d ", arr2[i]);
    }

    getch();
}
